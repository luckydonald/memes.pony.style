<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<link rel="shortcut icon" href="{{ static_prefix }}favicon.ico">
<link rel="stylesheet" href="{{ static_prefix }}css/responsive.css" type="text/css">
<link rel="stylesheet" href="{{ static_prefix }}css/normalize.css" type="text/css">
<link rel="stylesheet" href="{{ static_prefix }}css/rainbow_dash_always_dresses_in.css" type="text/css">
<link rel="stylesheet" href="{{ static_prefix }}css/rarity.css" type="text/css">
<link rel="stylesheet" href="{{ static_prefix }}css/jquery-ui-1.10.3.custom.css" type="text/css" media="all">
<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ static_prefix }}apple-touch-icon-144x144-precomposed.png">
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ static_prefix }}apple-touch-icon-114x114-precomposed.png">

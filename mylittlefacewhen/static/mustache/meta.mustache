<title>{{ title }} - MyLittleFaceWhen: My Little Pony reaction images</title>
<link rel="alternate" type="application/rss+xml" title="MyLittleFaceWhen.com latest images" href="http://MyLittleFaceWhen.com/feed/">
<meta name="viewport" content="width=device-width, height=device-height">        
<meta name="description" content="{{ description }}">
<meta property="og:title" content="MyLittleFaceWhen.com">
<meta property="og:type" content="website">
<meta property="og:url" content="http://mylittlefacewhen.com{{ path }}">
<meta id="og-image" property="og:image" content="{{ default_image }}">
<meta property="og:site_name" content="MyLittleFaceWhen.com">
{{# alt_image }}
<link rel="image_src" href="{{default_image}}">
{{/ alt_image }}

{{# canonical }}
<link rel="canonical" href="{{ canonical }}">
{{/ canonical }}

